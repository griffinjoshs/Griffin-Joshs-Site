<style>
  /* Navbar Scroll */
  .bg-dark {
    background-color: transparent !important;
  }

  /* Wave CSS */
  body {
    margin: 0;
    padding: 0;
  }


  .wave {
    background-color: transparent;
    position: relative;
  }

  .wave::before {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 150px;
    background: url(assets/images/curve.png);
  }

  /* contact handlebars */
  * {
    box-sizing: border-box;
  }

  #contCon {
    max-width: 1170px;
    margin-left: auto;
    margin-right: auto;
    padding: 1em;
  }

  #contactUl {
    list-style: none;
    padding: 0;
  }

  .brand {
    text-align: center;
  }

  .brand span {
    color: #fff;
  }

  #contWrap {
    box-shadow: 0 0 20px 0 rgba(72, 94, 116, 0.7);
  }

  #contWrap>* {
    padding: 1em;
  }

  .company-info {
    background: #c9e6ff;
  }

  .company-info h3,
  .company-info ul {
    text-align: center;
    margin: 0 0 1rem 0;
  }

  .contact {
    background: #f9feff;
  }

  /* FORM STYLES */
  .contact form {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-gap: 20px;
  }

  .contact form label {
    display: block;
  }

  .contact form p {
    margin: 0;
  }

  .contact form .full {
    grid-column: 1 / 3;
  }

  .contact form button,
  .contact form .four,
  .contact form textarea {
    width: 100%;
    padding: 1em;
    border: 1px solid #c9e6ff;
  }

  #subject {
    width: 205%;
    padding: 1em;
    border: 1px solid #c9e6ff;
  }

@media (max-width: 1150px){
  #subject {
    width: 150%;
  }
}

@media (max-width: 800px){
  #subject {
    width: 100%;
  }
}

  .contact form button:hover,
  .contact form button:focus {
    background: #92bde7;
    color: #fff;
    outline: 0;
    transition: background-color 2s ease-out;
  }

  /* LARGE SCREENS */
  @media(min-width:700px) {
    #contWrap {
      display: grid;
      grid-template-columns: 1fr 2fr;
    }

    #contWrap>* {
      padding: 2em;
    }

    .company-info h3,
    .company-info,
    .brand #contactUL {
      text-align: left;
    }
  }



  #email {
    height: 59px;
    width: 325px;
  }
</style>

